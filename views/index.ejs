<!DOCTYPE html>
<html>
    <head>
        <% include ./part/header %>
    </head>
    <body>
        <table id="table">
            <thead>
                <tr>
                    <th>NO</th>
                    <th>TITLE</th>
                    <th>ID</th>
                    <th>DATE</th>
                    <th>HIT</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach(function (board) { %>
                    <tr>
                        <td><%= board.no %></td>
                        <td><a href="/board/<%= board.no %>"><%= board.title %></a></td>
                        <td><%= board.id %></td>
                        <td><%= board.date %></td>
                        <td><%= board.hit %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>      
        <a href="./board">WRITE</a>
    </body>
</html>